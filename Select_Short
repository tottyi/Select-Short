#include <iostream>

using namespace std;

int main()
{
    int array[3] = {9,4,5};
    int tmp, j;
    
    for (int i = 3; i >= 0; i--)
    {
        tmp = array[i];
        j = i + 1;
        while (tmp > array[j] && j < 3)
        {
            array[j - 1] = array[j];
            j++;
        }
        array[j - 1] = tmp;
    }

    cout << endl;
    for (int i = 0; i < 3; i++)
    {
        cout << array[i] << " ";
    }

    cout << "\n\n";
    system("pause");
    return 0;
}
